# If you want to ignore temp files generated by your IDE or OS then add a global ignore:
#   git config --global core.excludesfile ~/.gitignore_global

# Ignore bundler config
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore all logfiles and tempfiles.
/log/**/*.log
/log/*.gz
/tmp/*

# Ignore local settings.
.env.*.local
*.local.yml
*.local.rb
config/apache*.conf

# Ignore IDE settings.
.idea/
*.iml
.history
.vscode
jsconfig.json

# Ignore .DS_Store files on Mac
.DS_Store

# Files that might appear on external disks
.Spotlight-V100
.Trashes

# Ignore the SASS cache
.sass-cache

# Ignore assets in the public directory - generated by the front-end build
public/index-main.html
public/index-junction.html
public/bcourses_embedded.html
public/rev-manifest.json
public/styleguide

# Ignore precompiled assets
/dist
/public/assets

# Ignore the coverage directory (for checking in, otherwise useful)
/coverage

# Node
/node_modules
npm-debug.log

# Ignore processed VCR files
/fixtures/vcr_cassettes/*.json

# Ignore git version stamp
/versions/git.txt

# Ignore installed gem checking
/vendor

# Ignore pesky Oracle jars
/lib/ojdbc*.jar

# Ignore files generated by jsdoc
/out

# Prettier ignore file
.prettierignore
